{
    "env": {
      "node": true
    },
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "globals": {
    },
    "plugins": [
    ],
    "extends": "eslint-config-airbnb",
    "rules": {
      "no-console": "off",
      "indent": [ "warn", 4 ],
      "quotes": [ "warn", "single" ],
      "linebreak-style": 0,
      "react/jsx-indent": [1, 4],
      "react/jsx-indent-props": [1, 4],
      "react/react-in-jsx-scope": 0,
      "react/prop-types": 0
    }
  }